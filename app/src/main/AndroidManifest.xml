<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.demo1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Controller.LoginRegistration.LoginActivity"
            android:label="@string/title_activity_login"></activity>
        <activity android:name=".Controller.LoginRegistration.HomeActivity" />
        <activity android:name=".Controller.LoginRegistration.CalendarActivity" />
        <activity android:name=".Controller.LoginRegistration.ResetPassActivity" />
        <activity android:name=".Controller.LoginRegistration.RegistrationActivity" />
        <activity android:name=".Controller.LoginRegistration.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Controller.LoginRegistration.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>